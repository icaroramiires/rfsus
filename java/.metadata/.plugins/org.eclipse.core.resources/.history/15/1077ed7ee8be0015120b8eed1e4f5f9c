package br.edu.ifba.rfsus.jna.rfid;

public class LeitorRfid implements Runnable {

	private boolean continuar = true;
	
	@Override
	public void run() {
		continuar = true;
		
		while (continuar) {
			
			
			try {
				Thread.sleep(1000);
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
		}
	}

}
